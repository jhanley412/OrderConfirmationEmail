<mjml>
  <mj-body background-color="#fffff" font-size="13px">
  <mj-wrapper border="1px solid #000" padding="0px">
    <mj-section background-color="#fff" padding-bottom="10px" padding-top="10px" border-top="10px solid #e3dfd6" border-bottom="2px solid #000">
      <mj-column width="50%">
        <mj-image src="https://d2aovoe6slpue4.cloudfront.net/stores/bb-logo.png" alt="BootBarn.com"></mj-image>
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding-bottom="0px" padding-top="0">
      <mj-column width="90%">
              <mj-text font-size="13px" color="#000000" font-family="Ubuntu, Helvetica, Arial, sans-serif" padding-left="25px" padding-right="25px" padding-bottom="10px" padding-top="10px">
                <p style="font-size:16px;">Hi {{FirstName}} {{LastName}}</p>
                <p style="font-size:16px;">Thank you for your order!</p>
                <p style="font-size:16px;">We'll follow up with another email once your items have shipped. Thank you for shopping with us!</p>
              </mj-text>
      </mj-column>
    </mj-section>
    <mj-section >
        <mj-column width="60%">
            <mj-image alt="Order Placed" width="800px" src="data:image/png;base64,"/>
        </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding-bottom="0px" padding-top="0">
          <mj-column width="90%">
                  <mj-text text-transform="uppercase" align="center" font-size="18px" color="#000000" font-family="Ubuntu, Helvetica, Arial, sans-serif" letter-spacing="1px" padding-left="25px" padding-right="25px" padding-bottom="10px" padding-top="10px">
                    Order &nbsp; Summary
                  </mj-text>
          </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding-bottom="0px" padding-top="0">
              <mj-column width="90%"  border-top="2px solid #e3dfd6" border-bottom="2px solid #e3dfd6">
                      <mj-text align="center" font-size="16px" color="#000000" font-family="Ubuntu, Helvetica, Arial, sans-serif" padding-left="25px" padding-right="25px" padding-bottom="0px" padding-top="0px" >
                        <p style="font-size:16px;">Order Number: {{Order Number}}</p>
                      </mj-text>
              </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding-bottom="0px" padding-top="10px">
                  <mj-column width="40%" vertical-align="top" padding-bottom="20px">
                          <mj-text font-size="14px" color="#000000" font-family="Ubuntu, Helvetica, Arial, sans-serif" padding-left="25px" padding-right="25px" padding-bottom="0px" padding-top="0px" >
                            <div style="font-weight:bold"><mj-text font-weight="400" font-size="16px">Ship To:</mj-text></div>
                            <div>
                            Trishan Singh<br/>
                            BootBarn<br/>
                            3211 Internet Blvd<br/>
                            STE 300<br/>
                            Frisco, TX 75034 USA<br/>
                            </div>
                          </mj-text>
                  </mj-column>
                  <mj-column width="40%" vertical-align="top" padding-bottom="20px">
                          <mj-text font-size="14px" color="#000000" font-family="Ubuntu, Helvetica, Arial, sans-serif" padding-left="25px" padding-right="25px" padding-bottom="0px" padding-top="0px" >
                            <div style="font-weight:bold"><mj-text font-weight="400" font-size="16px">Bill To:</mj-text></div>
                            <div>
                            Trishan Singh<br/>
                            BootBarn<br/>
                            3211 Internet Blvd<br/>
                            STE 300<br/>
                            Frisco, TX 75034 USA<br/>
                            </div>
                          </mj-text>
                  </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff">
                        <mj-column width="200px">
                            <mj-image  href="{p.url}" src="https://d5n0lf4gshqmd.cloudfront.net/img/034B01_89_p1/200/250/i.jpg" width="200px"></mj-image>
                        </mj-column>
                        <mj-column padding-left="10px" padding-right="10px">
                            <mj-text padding-left="0" padding-right="0" letter-spacing="1.1px"  font-size="14px" font-weight="bold" text-transform="uppercase"> Tony Lama Men's Vintage Full Quill Ostrich Boots - Medium Toe </mj-text>
                            <mj-text padding-left="0" padding-right="0" letter-spacing="1px"  font-size="12px">
                                <div>
                                    <span style="text-transform:uppercase;color:#a0a0a0">Style: 097653234567 </span><br/>
                                    Size: {{item.Size}} <br/>
                                    Color: {{item.Color}} <br/>
                                    Qty: 1
                                </div>
                            </mj-text>
                        </mj-column>
                        <mj-column width="100px" padding-left="10px" padding-right="10px">
                            <mj-text padding-left="0" padding-right="0" letter-spacing="1px"  font-size="16px" text-transform="uppercase"> $165.99 </mj-text>
                        </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff">
                        <mj-column width="200px">
                            <mj-image  href="{p.url}" src="https://d5n0lf4gshqmd.cloudfront.net/img/034B01_89_p1/200/250/i.jpg" width="200px"></mj-image>
                        </mj-column>
                        <mj-column padding-left="10px" padding-right="10px">
                            <mj-text padding-left="0" padding-right="0" letter-spacing="1.1px"  font-size="14px" font-weight="bold" text-transform="uppercase"> Tony Lama Men's Vintage Full Quill Ostrich Boots - Medium Toe </mj-text>
                            <mj-text padding-left="0" padding-right="0" letter-spacing="1px"  font-size="12px">
                                <div>
                                    <span style="text-transform:uppercase;color:#a0a0a0">Style: 097653234567 </span><br/>
                                    Size: {{item.Size}} <br/>
                                    Color: {{item.Color}} <br/>
                                    Qty: 1
                                </div>
                            </mj-text>
                        </mj-column>
                        <mj-column width="100px" padding-left="10px" padding-right="10px">
                            <mj-text padding-left="0" padding-right="0" letter-spacing="1px"  font-size="16px" text-transform="uppercase"> $165.99 </mj-text>
                        </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff" padding-bottom="10px" padding-top="10px" border-top="5px solid #e3dfd6" >
      <mj-column width="50%">
                      <mj-text padding-left="0" padding-right="0" letter-spacing="1.2px"  font-size="19px" font-weight="bold" text-transform="uppercase"> &nbsp; </mj-text>
      </mj-column>
      <mj-column width="50%">
        <mj-text>Subtotal : {{Subtotal}}</mj-text>
        <mj-text>Shipping ( Ground ) : {{Shipping}}</mj-text>
        <mj-text>Tax : {{Tax}}</mj-text>
        <mj-text>Discount : - </mj-text>
        <mj-text>Total : {{Total}}</mj-text>
      </mj-column>
    </mj-section>
    <mj-section background-color="#ffffff">

        <mj-column width="80%" border="10px solid #e3dfd6" >
            <mj-text align="center" text-transform="uppercase" >
            <div>You may also like</div>
            <div style="display:inline-block;width:30%;vertical-align:top">
                        <a href="href="{{rec.url}}">
                            <img  src="https://d5n0lf4gshqmd.cloudfront.net/img/034B01_89_p1/200/250/i.jpg" width="100%"/>
                        </a>
                        <div style="text-align:left">{{rec.productname}}</div>
                        <div style="text-align:left;color:red">{{p.price}}</div>
            </div>
            <div style="display:inline-block;width:30%;vertical-align:top">
                        <a href="href="[[p.url]">
                            <img  src="https://d5n0lf4gshqmd.cloudfront.net/img/034B01_89_p1/200/250/i.jpg" width="100%"/>
                        </a>
                        <div style="text-align:left">My Product Title</div>
                        <div style="text-align:left;color:red">$199.99</div>
            </div>
            <div style="display:inline-block;width:30%;vertical-align:top">
                         <a href="href="{p.url}">
                             <img  src="https://d5n0lf4gshqmd.cloudfront.net/img/034B01_89_p1/200/250/i.jpg" width="100%"/>
                         </a>
                         <div style="text-align:left">My Product Title</div>
                         <div style="text-align:left;color:red">$199.99</div>
            </div>


            </mj-text>

        </mj-column>

    </mj-section>

    <mj-section background-color="#ffffff" background-url="https://d2aovoe6slpue4.cloudfront.net/NewSite/HomePageBanners/2018_HomepageASlot_WWS.jpg">

            <mj-column width="80%" border="10px solid #ffffff" background-color="#ffffff"  >
                <mj-text align="center" text-transform="uppercase" >
                <div style="font-size:18px;font-weight:bold">B-Rewards Loyalty Program</div>
                </mj-text>
                <mj-text align="center">
                <p>
                Recieve exclusive discounts promotions and more from BootBarn<br/>
                Get 150 pts just for signing up
                </p>
                </mj-text>


                <mj-button background-color="#9b1a30" color="#FFF" font-size="14px" align="center" font-weight="bold" border="none" padding="15px 30px" border-radius="10px" href="https://mjml.io" font-family="Helvetica" padding-left="25px" padding-right="25px" padding-bottom="10px"
                                          padding-top="10px">Sign In</mj-button>
                <mj-button background-color="#9b1a30" color="#FFF" font-size="14px" align="center" font-weight="bold" border="none" padding="15px 30px" border-radius="10px" href="https://mjml.io" font-family="Helvetica" padding-left="25px" padding-right="25px" padding-bottom="10px"
                                          padding-top="10px">Join Now</mj-button>
            </mj-column>

        </mj-section>



    <mj-section background-color="#ffffff" padding-bottom="5px" padding-top="0">
      <mj-column width="100%">
        <mj-divider border-color="#6c4041" border-width="2px" border-style="solid" padding-left="20px" padding-right="20px" padding-bottom="0px" padding-top="0"></mj-divider>
        <mj-social font-size="15px" icon-size="30px" mode="horizontal">
          <mj-social-element name="facebook" href="https://mjml.io/">
            Facebook
          </mj-social-element>
          <mj-social-element name="instagram" href="https://mjml.io/">
            Instagram
          </mj-social-element>
          <mj-social-element  name="twitter" href="https://mjml.io/">
            Twitter
          </mj-social-element>
        </mj-social>
        <mj-text align="center" color="#6c4041" font-size="15px" font-family="Helvetica" padding-left="25px" padding-right="25px" padding-bottom="10px" padding-top="10px">
          <div style="font-size:15px">Thank You!</div>
          <div style="font-size:15px"><a href="mailto:{{customerserviceemail}}">{{customerserviceemail}}</a></div>
          <div style="font-size:15px"><a href="tel:{{customerservicephone}}">{{customerservicephone}}</a></div>

        </mj-text>
      </mj-column>
    </mj-section>
  </mj-wrapper>
  </mj-body>
</mjml>